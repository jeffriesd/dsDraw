define f() { 
  b = bst([8, 5, 13, 4, 1, 0, 22, 19]);

  node = b[5];
  while (node != null) {
    node.bg = "red";
    repaint();
    wait(100); 
    node = node.parent();
  }
}

define colorPath() {
  b = bst([8, 6, 13, 4, 1, 0, 22, 19]);
  repaint();
  wait(1000);
  node = b[5];
  while (node != null) {
    node.bg = "red";
    node = node.parent();
  }
}

define f() { 
  b = bst();
  b.asdf();
}

